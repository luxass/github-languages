{
  "name": "@luxass/github-languages",
  "version": "2.0.14",
  "description": "GitHub's supported languages",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.17.1",
  "license": "MIT",
  "homepage": "https://github.com/luxass/github-languages",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/github-languages.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/github-languages/issues"
  },
  "keywords": [
    "languages",
    "linguist",
    "github",
    "github languages"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./languages": {
      "import": "./dist/languages.js",
      "require": "./dist/languages.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "languages.d.ts",
    "languages.json"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsdown",
    "languages:download": "tsx --env-file=.env ./scripts/download.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@luxass/eslint-config": "5.3.2",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.18.6",
    "eslint": "^9.35.0",
    "eslint-plugin-format": "^1.0.1",
    "js-yaml": "4.1.0",
    "publint": "0.3.13",
    "tsdown": "0.15.5",
    "tsx": "4.20.6",
    "typescript": "5.9.2",
    "valibot": "1.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
